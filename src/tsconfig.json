{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2017",
    "module": "ESNext",
    "sourceMap": true,
    "declaration": false,
    "importHelpers": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "outDir": "./build/dist",
    "lib": ["es2017", "es5", "dom"],
    "allowSyntheticDefaultImports": true,
    "types": ["react", "node"],
    "baseUrl": "./",
    "paths": {
      "*": [
        "*"
      ]
    }
  },
  "include": [
    "../node_modules/@types/google.maps/index.d.ts",
    "**/*"
  ]
}